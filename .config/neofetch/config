# Info to print
print_info() {
	info title
	info underline
	info "OS" distro
	info "Model" model
	info "Resoulution" resolution
	info "CPU" cpu
	info "GPU" gpu
	info "RAM" memory
	info "Kernel" kernel
	info "Packages" packages
	info "Shell" shell
	info "WM" wm
	info "Theme" theme
	info "Icons" icons
	info "Term" term
	info "Term Font" term_font
	info "Font" font
	info cols
}

